<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Food market{% endblock %}</title>
    <link rel="icon"
          href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        <link rel="stylesheet" type="text/css" href="{{ asset('css/fdm.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('css/components.css') }}">
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
<h1 style="text-align: center"> Food Market</h1>
{% block nav %}
    <a class="button import" href="{{ path('homepage') }}"> Home </a>
    <a class="button import" href="{{ path('product_import') }}"> Import products </a>
{% endblock %}
{% block body %}

{% endblock %}
</body>
</html>
